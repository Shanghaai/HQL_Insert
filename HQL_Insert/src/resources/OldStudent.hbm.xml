<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="beans.OldStudent" table="oldstudent" schema="aon">
		<id name="id" column="OldID">
			<generator class="increment"></generator>
		</id>
		<property name="name" column="OldName"></property>
		<property name="address" column="Oldaddress"></property>

	</class>
</hibernate-mapping>
